{
  "name": "swaglabs",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf cypress/reports/mocha",
    "pretest": "npm run clean",
    "scripts": "cypress run",
    "combine-reports": "mochawesome-merge cypress/reports/mocha/*.json > cypress/reports/mochareports/report.json",
    "generate-report": "marge cypress/reports/mochareports/report.json -f report -o cypress/reports/mochareports",
    "posttest": "npm run combine-reports && npm run generate-report && npm run report",
    "report": "open-cli cypress/reports/mochareports/report.html",
    "test": "npm run scripts || npm run posttest"
  },
  "keywords": ["Cypress","Cucumber","e2e","Automation"],
  "author": "Leonardo Menezes",
  "license": "ISC",
  "dependencies": {
    "chance": "^1.1.8",
    "faker": "^6.6.6",
    "faker-br": "^0.4.1",
    "node": "^16.14.2"
  },
  "devDependencies": {
    "@fakerjs/faker": "^3.0.0",
    "cypress": "^9.5.2",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-localstorage-commands": "^1.7.0",
    "cypress-multi-reporters": "^1.5.0",
    "cypress-terminal-report": "^3.4.2",
    "cypress-timings": "^1.0.0",
    "mocha": "^9.2.2",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.2.0",
    "open-cli": "^7.0.1",
    "typescript": "^4.6.3"
  },
  "description": "\u0000",
  "cypress-cucumber-preprocessor": {
    "step_definitions": "cypress/support/steps",
    "nonGlobalStepDefinitions": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LeonardoRmenezes/swaglabs"
  }
}